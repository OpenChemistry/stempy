   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "inner_radii = [0, 40]\n",
    "outer_radii = [288, 288]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "reader = io.reader(files)\n",
    "imgs = image.create_stem_images(reader, inner_radii, outer_radii, width=160, height=160)"
    "imgs = [np.array(img, copy=False) for img in imgs]"
    "imgs = np.array(imgs, copy=False)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for img in imgs:\n",
    "    fig,ax=plt.subplots(figsize=(6,6))\n",
